<%= form_with(url: '/signin', method: "post") do %>
<%= label_tag :name, "Your name: "%>
<%= collection_select(:user, :name, @users, :name, :name) %> <br>
<%# can't get an id in the html if i use the form builder on collection select. %>
<%= label_tag :password, "Your password: "%>
<%= password_field_tag :password %> <br>
<%= submit_tag "Sign In" %>
<% end %>